    "#Módulo de operações matemáticas\n",
    "def soma(x,y):\n",
    "    return x + y\n",
    "\n",
    "def subtrat(x,y):\n",
    "    return x - y\n",
    "\n",
    "def multiply(x,y):\n",
    "    return x * y\n",
    "\n",
    "def divide(x,y):\n",
    "    if y != 0:\n",
    "        return x / y\n",
    "    else:\n",
    "        raise ValueError('Não é permitido fazer divisão por zero')"
